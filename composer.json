{
    "name": "voltel/lorem-ipsum-bundle",
    "description": "Happy \"Lorem ipsum\" text with positive English words",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Telnov",
            "email": "volodymyr.telnov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/dependency-injection": "^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "symfony/framework-bundle" : "^4.0",
        "symfony/browser-kit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "KnpU\\LoremIpsumBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["tests/lib/functions.php"],
        "psr-4": {
            "KnpU\\LoremIpsumBundle\\Tests\\": "tests/"
        }
    }
}
